<div class="nav-top">
<%# go-back-icon %>
<div class="back"><%= link_to 'javascript:history.back()' do %>
<i class="fa-solid fa-angle-left"></i>
<% end %></div>
<%= image_tag "www-logo.png", alt: "Logo", class: "logo-img" %>
</div>

<h1><%= @restaurant.name %>'s menu</h1>

<%# card for each food items %>
<ul>
  <% @restaurants.each do |restaurant| %>
    <li>Food item : <%= restaurant.name %> </li>
    <li>Price : $ <%= restaurant. %> </li>
    <li>Description : <%= restaurant.description %> </li>
    <li>Quantity : <%= restaurant.quantity %> </li>
  <br>
  <% end %>
</ul>
